{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["<rootDir>/jest-e2e.setup.ts"],
  "globalSetup": "<rootDir>/jest-global-setup.ts",
  "globalTeardown": "<rootDir>/jest-global-teardown.ts",
  "testTimeout": 60000,
  "maxWorkers": 1,
  "forceExit": true
}
